<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="447.0" prefWidth="619.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.verbund.pwmanager.gui.managerController">
    <children>
        <VBox layoutX="30.0" layoutY="14.0" prefHeight="404.0" prefWidth="560.0">
            <children>
                <HBox prefHeight="34.0" prefWidth="560.0" spacing="10.0">
                    <children>
                        <Label prefHeight="17.0" prefWidth="52.0" text="Name: " />
                        <TextField fx:id="textName" />
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </HBox>
                <HBox prefHeight="38.0" prefWidth="560.0" spacing="10.0">
                    <children>
                        <Label text="Benutzer: " />
                        <TextField fx:id="textUser" />
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </HBox>
                <HBox prefHeight="33.0" prefWidth="560.0" spacing="10.0">
                    <children>
                        <Label text="Passwort: " />
                        <PasswordField fx:id="textPasswort" />
                        <Button mnemonicParsing="false" onAction="#generate" prefHeight="25.0" prefWidth="129.0" text="Passwort generieren" />
                        <Button mnemonicParsing="false" onAction="#einfuegen" text="Passwort einfuegen" />
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#eintragen" text="Eintragen" />
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </HBox>
                <HBox prefHeight="45.0" prefWidth="560.0" spacing="10.0">
                    <children>
                        <TextField fx:id="textSuche" />
                        <Button mnemonicParsing="false" onAction="#suche" text="Suche" />
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </HBox>
                <TextArea fx:id="textAusgabe" editable="false" prefHeight="251.0" prefWidth="540.0">
                    <VBox.margin>
                        <Insets bottom="10.0" />
                    </VBox.margin></TextArea>
                <HBox alignment="CENTER_RIGHT" prefHeight="63.0" prefWidth="540.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#listeLeeren" text="Liste leeren" />
                    </children>
                    <opaqueInsets>
                        <Insets bottom="10.0" top="10.0" />
                    </opaqueInsets>
                </HBox>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </VBox>
    </children>
</AnchorPane>
